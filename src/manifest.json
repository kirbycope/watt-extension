{
	"manifest_version": 2,
	"name": "WATT - Web Automation Testing Tool",
	"short_name": "WATT",
	"version": "0.1.0",
	"description": "A featureful website testing tool.",
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"<all_urls>"
		,"activeTab"
		,"browsingData"
		,"cookies"
		,"tabs"
		,"storage"
	],
	"browser_action": {
		"default_icon": "icon.png"
		,"default_title": "WATT"
		,"default_popup": "extension.html"
	},
	"options_ui": {
		"page": "options.html",
		"chrome_style": true
	}
}